	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#8C7A66",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Keywords and Control Flow",
			"scope": [
				"keyword",
				"keyword.control",
				"keyword.operator",
				"storage"
			],
			"settings": {
				"foreground": "#E4B027"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"string.regexp"
			],
			"settings": {
				"foreground": "#8BAA8B"
			}
		},
		{
			"name": "Variables and Parameters",
			"scope": [
				"variable",
				"variable.parameter",
				"variable.interpolation"
			],
			"settings": {
				"foreground": "#E8E2D9"
			}
		},
		{
			"name": "Functions and Methods",
			"scope": [
				"entity.name.function",
				"support.function",
				"support.function.any-method"
			],
			"settings": {
				"foreground": "#DC9562"
			}
		},
		{
			"name": "Types and Classes",
			"scope": [
				"entity.name.type",
				"entity.name.class",
				"entity.name.type.class",
				"support.class"
			],
			"settings": {
				"foreground": "#AAB276"
			}
		},
		{
			"name": "Constants and Numerics",
			"scope": [
				"constant",
				"constant.variable",
				"constant.numeric"
			],
			"settings": {
				"foreground": "#D1885F"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation.definition.comment",
				"punctuation.definition.method-parameters",
				"punctuation.definition.function-parameters",
				"punctuation.definition.parameters",
				"punctuation.definition.separator",
				"punctuation.definition.array"
			],
			"settings": {
				"foreground": "#8C7A66"
			}
		},
		{
			"name": "Imports and Packages",
			"scope": [
				"storage.modifier.package",
				"storage.modifier.import"
			],
			"settings": {
				"foreground": "#F8F2E9"
			}
		},
		{
			"name": "Escape Characters",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#8BAA8B"
			}
		},
		{
			"name": "Markup Headings",
			"scope": [
				"markup.heading"
			],
			"settings": {
				"foreground": "#E4B027"
			}
		},
		{
			"name": "Markup Bold",
			"scope": [
				"markup.bold"
			],
			"settings": {
				"foreground": "#E4B027",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup Italic",
			"scope": [
				"markup.italic"
			],
			"settings": {
				"foreground": "#DC9562",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup Links",
			"scope": [
				"markup.link"
			],
			"settings": {
				"foreground": "#AAB276"
			}
		},
		{
			"name": "Errors",
			"scope": [
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#F8F2E9",
			}
		},
		{
			"name": "Warnings (Deprecated)",
			"scope": [
				"invalid.deprecated"
			],
			"settings": {
				"foreground": "#F8F2E9",
			}
		},
		{
    "name": "HTML Tags",
    "scope": [
      "entity.name.tag.html"
    ],
    "settings": {
      "foreground": "#C1784F"
    }
  },
  {
    "name": "HTML Attributes",
    "scope": [
      "entity.other.attribute-name.html"
    ],
    "settings": {
      "foreground": "#8A9256"
    }
  },
  {
    "name": "DOCTYPE",
    "scope": [
      "meta.tag.sgml.doctype.html"
    ],
    "settings": {
      "foreground": "#E4B027"
    }
  },
  {
    "name": "ERB Tags",
    "scope": [
      "punctuation.section.embedded.ruby"
    ],
    "settings": {
      "foreground": "#8C7A66"
    }
  },
  {
    "name": "HTML Entities",
    "scope": [
      "constant.character.entity.html"
    ],
    "settings": {
      "foreground": "#D1885F"
    }
  },
  {
    "name": "HTML Punctuation",
    "scope": [
      "punctuation.definition.tag.html",
      "punctuation.separator.key-value.html"
    ],
    "settings": {
      "foreground": "#8C7A66"
    }
  }
	]
}